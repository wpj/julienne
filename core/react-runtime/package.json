{
  "name": "@julienne/react-runtime",
  "version": "0.1.1",
  "description": "React runtime module for julienne sites",
  "type": "module",
  "source": "./src/index.tsx",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && microbundle build --format es,modern,cjs --jsx React.createElement",
    "lint:ts": "eslint --max-warnings 0 '**/*.tsx'",
    "lint": "npm run lint:ts",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wpj/julienne.git"
  },
  "author": "Wyatt Johnston",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wpj/julienne/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/wpj/julienne/core/react-runtime#readme",
  "devDependencies": {
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.28.0",
    "microbundle": "^0.13.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@julienne/runtime": "workspace:>=0.1.1"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "volta": {
    "node": "14.17.1",
    "npm": "7.17.0"
  }
}

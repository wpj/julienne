{
  "name": "julienne",
  "version": "0.2.0",
  "description": "Small static site generator",
  "repository": "wpj/julienne",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": "Wyatt Johnston",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && npm run build-js && npm run build-types",
    "build-js": "rollup -c --silent",
    "build-types": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "lint:ts": "eslint --max-warnings 0 '**/*.ts'",
    "lint": "npm run lint:ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "watch-js": "rollup -c --watch"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.4",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@julienne/runtime": "workspace:^0.1.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/babel__core": "^7.1.14",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^15.12.1",
    "@types/rimraf": "^3.0.0",
    "@types/serve-handler": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-jest": "^27.0.2",
    "eslint": "^7.28.0",
    "jest": "^27.0.4",
    "jest-circus": "^27.0.4",
    "jest-environment-node": "^27.0.3",
    "jest-playwright-preset": "^1.6.0",
    "jest-runner": "^27.0.4",
    "playwright": "~1.11.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.0",
    "serve-handler": "^6.1.3",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@tinyhttp/app": "^1.3.7",
    "aggregate-error": "^3.1.0",
    "fs-extra": "^10.0.0",
    "is-stream": "^2.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.31",
    "vite": "^2.3.6"
  },
  "volta": {
    "node": "15.14.0",
    "npm": "7.16.0"
  }
}

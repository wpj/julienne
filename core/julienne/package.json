{
  "name": "julienne",
  "version": "0.2.0",
  "description": "Small static site generator",
  "repository": "wpj/julienne",
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "author": "Wyatt Johnston",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && npm run build-js && npm run build-types",
    "build-js": "julienne-build build",
    "build-types": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "lint:ts": "eslint --max-warnings 0 '**/*.ts'",
    "lint": "npm run lint:ts",
    "test": "node --loader esbuild-node-loader node_modules/uvu/bin.js tests",
    "typecheck": "tsc --noEmit",
    "watch-js": "julienne-build watch"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@julienne/build": "workspace:*",
    "@types/fs-extra": "^9.0.11",
    "@types/jsdom": "^16.2.12",
    "@types/node": "^15.12.1",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "esbuild-node-loader": "^0.1.1",
    "eslint": "^7.28.0",
    "jsdom": "^16.6.0",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "rimraf": "^3.0.2",
    "rollup": "^2.51.1",
    "testdouble": "^3.16.1",
    "typescript": "^4.3.2",
    "uvu": "^0.5.1"
  },
  "dependencies": {
    "aggregate-error": "^3.1.0",
    "fs-extra": "^10.0.0",
    "is-stream": "^2.0.0",
    "vite": "^2.4.2"
  },
  "volta": {
    "node": "14.17.1",
    "npm": "7.19.0"
  }
}

{
  "name": "@julienne/svelte-runtime",
  "version": "0.0.0",
  "description": "Svelte runtime for julienne sites",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "microbundle build --format es,cjs",
    "lint:ts": "eslint --max-warnings 0 '**/*.ts'",

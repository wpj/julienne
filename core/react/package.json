{
  "name": "@julienne/react",
  "version": "0.2.0",
  "description:": "React configuration and server renderer for julienne sites",
  "type": "module",
  "source": "./src/index.ts",
  "main": "./dist/index.cjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && npm run build-js && npm run build-types",
    "build-js": "julienne-build build",
    "build-types": "tsc --emitDeclarationOnly",
    "lint:ts": "eslint --max-warnings 0 '**/*.tsx'",
    "lint": "npm run lint:ts",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wpj/julienne.git"
  },
  "author": "Wyatt Johnston",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wpj/julienne/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/wpj/julienne/core/react-render#readme",
  "devDependencies": {
    "@julienne/build": "workspace:*",
    "@types/node": "^15.12.1",
    "@types/prettier": "^2.2.3",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.28.0",
    "julienne": "workspace:^0.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "prettier": "^2.3.1"
  },
  "peerDependencies": {
    "@julienne/react-runtime": "^0.1.1",
    "julienne": "workspace:^0.2.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "volta": {
    "node": "14.17.1",
    "npm": "7.17.0"
  }
}

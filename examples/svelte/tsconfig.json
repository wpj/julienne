{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": ["src", "types", "app.js", "build.js", "config.js"],
  "exclude": ["node_modules", ".julienne"],
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "esnext",
    "skipLibCheck": true,
    "strict": true
  }
}

{
  "name": "example-svelte",
  "version": "0.0.0",
  "private": true,
  "description": "Example svelte app using julienne",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node -r source-map-support/register app.js",
    "build": "node -r source-map-support/register build.js",
    "lint:svelte": "svelte-check",
    "lint:ts": "eslint --max-warnings 0 '**/*.{js,ts,ts,tsx}'",
    "lint": "npm run lint:ts && npm run lint:svelte",
    "start": "node -r source-map-support/register app.js",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.12",
    "@tsconfig/svelte": "^2.0.1",
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.28.0",
    "julienne": "workspace:^0.4.0",
    "rollup": "^2.52.3",
    "source-map-support": "^0.5.19",
    "svelte": "^3.38.3",
    "svelte-check": "^2.2.0",
    "svelte-preprocess": "^4.7.4",
    "typescript": "^4.3.2",
    "vite": "^2.4.2"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.4.5",
    "express": "^4.17.1",
    "sirv": "^1.0.12"
  },
  "volta": {
    "node": "14.17.1",
    "npm": "7.17.0"
  }
}

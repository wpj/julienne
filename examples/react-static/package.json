{
  "name": "example-react-static",
  "version": "0.0.0",
  "private": true,
  "description": "Example react app using julienne",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node -r source-map-support/register --es-module-specifier-resolution=node app.js",
    "build": "node -r source-map-support/register build.js",
    "lint:ts": "eslint --max-warnings 0 '**/*.{js,ts,jsx,tsx}'",
    "lint": "npm run lint:ts",
    "start": "node -r source-map-support/register --es-module-specifier-resolution=node app.js",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-helmet": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "eslint": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "julienne": "workspace:^0.2.0",
    "source-map-support": "^0.5.19",
    "typescript": "^4.3.2",
    "vite": "^2.4.2"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.4.5",
    "classcat": "^5.0.3",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sirv": "^1.0.12"
  },
  "volta": {
    "node": "14.17.1",
    "npm": "7.17.0"
  }
}
